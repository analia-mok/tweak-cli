{
    "name": "AnaliaMok/tweak-cli",
    "description": "An opinionated bootstrapper tool for setting up Lando-based projects.",
    "keywords": ["tweak", "lando", "php", "cli"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/AnaliaMok/tweak-cli/issues",
        "source": "https://github.com/AnaliaMok/tweak-cli"
    },
    "authors": [
        {
            "name": "Analia Mok",
            "email": "amok@geekhive.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laminas/laminas-text": "^2.7",
        "laravel-zero/framework": "^7.0",
        "spatie/laravel-enum": "^1.4",
        "symfony/yaml": "^5.0",
        "titasgailius/terminal": "^0.2.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/tweak"]
}
